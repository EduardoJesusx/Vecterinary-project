<%= render :partial => "partials/header/header" %>

<p style="color: green"><%= notice %></p>

<h1>Pet owners</h1>

<div id="pet_owners">
  <% @pet_owners.each do |pet_owner| %>
    <%= render pet_owner %>
    <p>
      <%= link_to "Show this pet owner", pet_owner %>
    </p>
  <% end %>
</div>

<%= link_to "New pet owner", new_pet_owner_path %>
