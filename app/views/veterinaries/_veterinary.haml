%tr{ id: dom_id(veterinary) }
  %td
    %strong First name:
    = veterinary.first_name

  %td
    %strong Last name:
    = veterinary.last_name

  %td
    %strong Tipo:
    = veterinary.admin ? "Admin" : "Comum"

  - if params[:id] == nil
    %td 
      .link-button
        = link_to "Show this veterinary", veterinary
